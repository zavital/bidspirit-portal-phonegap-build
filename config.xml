<?xml version='1.0' encoding='utf-8'?>
<widget 
    id="com.bidspirit.qa"
    version="1.3.4" 
    xmlns="http://www.w3.org/ns/widgets" 
    xmlns:gap="http://phonegap.com/ns/1.0"
    >
 
    <name>(Qa) Bidspirit</name>
    <description>
        The Bidspirit portal is your gate to the auction houses’ arena in Israel trading in art, collectibles, jewelry, antiques, stamps, coins, old books, judaica, and more
    </description>
    <author email="zemer@bidspirit.com" href="http://bidspirit.com">
        WOMmee Team
    </author>

    <content src="index.html" />
    <access origin="*" />
    

    <gap:splash src="splash.png" />
    <icon src="icon.png" />

       
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="fullscreen" value="false" />
    <preference name="webviewbounce" value="false" />
    <preference name="DisallowOverscroll" value="true"/>
    <preference name="AutoHideSplashScreen" value="false"/>ß
    <preference name="SplashScreen" value="splash" />
    <preference name="SplashScreenDelay" value="20000" />
    <preference name="orientation" value="portrait" />
    <preference name="KeyboardShrinksView" value="false" />
    
</widget>
